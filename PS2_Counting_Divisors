/All Accepted

#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
    while(n--)
    {
        int x;
        cin >> x;
        int ctr = 1,flag = 0;
        for(int i = 2;i*i<=x;i++)
        {
            if(x%i == 0)
            {
                ctr++;
            }
            if(i*i == x)
            {
                flag = 1;
            }
        }
        
        if(flag)
        {
            cout << 2*ctr - 1;
        }
        else
        {
            if(x == 1)
            {
                cout << "1";
            }
            else
            {
                cout << 2*ctr;
            }
        }
        //cout << flag;
        cout << endl;
    }
    return 0;
}
