// 27/40 Accepted

#pragma GCC optimize("Ofast")
#pragma GCC optimize("unroll-loops")

#include <iostream>
#include <bits/stdc++.h>

using namespace std;

using ll = long long;
#define int ll

#define endl '\n'
#define fora(i, a, b) for (int i = int(a); i < int(b); i++)
#define foro(i, b) fora(i, 0, b)
#define forta(i, a, b) for (int i = int(b) - 1; i >= int(a); i--)
#define forto(i, b) forta(i, 0, b)
#define pb push_back
#define ff first
#define sda second
#define all(x) x.begin(), x.end()

using pi = pair<int, int>;
using vi = vector<int>;
using vvi = vector<vi>;

#define mp make_pair
#define mt make_tuple

void init_code()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}

void solve()
{
    int n;
    cin >> n;
    string x, y;
    cin >> x >> y;
    vi a(n, -1);
    int l = 0;
    int r1 = 0, r2 = 0;
    int ctr = 0;
    foro(i, n)
    {
        if (y[i] == 'C')
        {
            if (x[i] != 'C')
            {
                cout << "No";
                return;
            }
        }
    }
    int ctr1 = 0;
    int ctr2 = 0;
    foro(i, n)
    {
        if (y[i] == 'C')
        {
            if ((ctr2 + ctr < ctr1))
            {
                cout << "No";
                return;
            }
            if (ctr2 == ctr1)
            {
                if (l > r1)
                {
                    cout << "No";
                    return;
                }
            }
            if (ctr2 < ctr1)
            {
                if (l > r2)
                {
                    cout << "No";
                    return;
                }
            }

            ctr = 0;
            ctr1 = 0;
            ctr2 = 0;
            continue;
        }
        if (y[i] == 'B')
        {
            l = i;
            ctr1++;
        }
        if (x[i] == 'B')
        {
            r1 = i;
            ctr2++;
        }
        if (x[i] == 'C')
        {
            r2 = i;
            ctr++;
        }
        if (ctr2 > ctr1)
        {
            cout << "No";
            return;
        }
    }
    if ((ctr2 + ctr < ctr1))
    {
        cout << "No";
        return;
    }
    if (ctr2 != 0 && ctr2 == ctr1)
    {
        if (l > r1)
        {
            cout << "No";
            return;
        }
    }
    if (ctr2 < ctr1)
    {
        if (l > r2)
        {
            cout << "No";
            return;
        }
    }
    cout << "Yes";
}

signed main()
{

    init_code();

    int t = 1;
    cin >> t;
    while (t--)
    {
        solve();
        cout << endl;
    }
}
