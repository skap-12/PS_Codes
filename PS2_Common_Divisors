// All Accepted

#pragma GCC optimize ("Ofast")
#pragma GCC optimize ("unroll-loops")
 
#include <bits/stdc++.h>
using namespace std;
 
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin >> n;
    int x[n];
    for(int i = 0;i<n;i++)
    {
        cin >> x[i];
    }
    int r = *max_element(x,x+n);
    vector<int>chk(r+1,0);
    for(int j = 0;j<n;j++)
    {
        for(int i = 1;i*i<=x[j];i++)
        {
            if(x[j]%i == 0)
            {
                chk[i]++;
                chk[x[j]/i]++;
            }
            if(i*i == x[j])
            {
                chk[i]--;
            }
        }
    }
    for(int i = r;i>0;i--)
    {
        if(chk[i] > 1)
        {
            cout << i;
            break;
        }
    }
    return 0;
